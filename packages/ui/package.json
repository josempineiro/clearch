{
  "name": "@clearq/ui",
  "version": "1.0.0",
  "description": "Front end clean arquitecture UI library",
  "private": false,
  "main": "./dist/clearqui.umd.js",
  "module": "./dist/clearqui.es.js",
  "exports": {
    ".": {
      "import": "./dist/clearqui.es.js",
      "require": "./dist/clearqui.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "env": {
    "browser": true,
    "amd": true,
    "node": true
  },
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build:lib": "storybook build",
    "build": "tsc --outDir dist && vite build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,mdx}\"",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@clearq/core": "^1.0.0",
    "classnames": "^2.3.2",
    "framer-motion": "^10.12.18",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@clearq/eslint-config": "*",
    "@storybook/addon-essentials": "7.0.23",
    "@storybook/addon-interactions": "7.0.23",
    "@storybook/addon-links": "7.0.23",
    "@storybook/addon-styling": "1.3.0",
    "@storybook/blocks": "7.0.23",
    "@storybook/react": "7.0.23",
    "@storybook/react-vite": "7.0.23",
    "@storybook/testing-library": "0.2.0",
    "@testing-library/react": "14.0.0",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "4.0.1",
    "@vitest/coverage-c8": "0.32.2",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.2",
    "jsdom": "22.1.0",
    "json": "11.0.0",
    "lint-staged": "13.2.2",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prop-types": "15.8.1",
    "storybook": "7.0.23",
    "storybook-addon-docs-tabs": "^1.0.3",
    "typescript": "^5.1.6",
    "vite": "4.3.9",
    "vite-plugin-dts": "2.3.0",
    "vitest": "0.32.2"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier -w"
    ]
  }
}
