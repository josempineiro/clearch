{
  "name": "@clearq/core",
  "version": "1.0.0",
  "description": "Front end clean arquitecture core components library",
  "private": false,
  "main": "./dist/clearqcore.umd.js",
  "module": "./dist/clearqcore.es.js",
  "exports": {
    ".": {
      "import": "./dist/clearqcore.es.js",
      "require": "./dist/clearqcore.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "env": {
    "browser": true,
    "amd": true,
    "node": true
  },
  "engines": {
    "node": "18"
  },
  "scripts": {
    "build": "tsc --outDir dist && vite build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,mdx}\"",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "framer-motion": "^10.12.18",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@testing-library/react": "14.0.0",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@vitejs/plugin-react": "4.0.1",
    "@vitest/coverage-c8": "0.32.2",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.2",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "35.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-storybook": "0.6.12",
    "jsdom": "22.1.0",
    "json": "11.0.0",
    "lint-staged": "13.2.2",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prop-types": "15.8.1",
    "typescript": "^5.1.6",
    "vite": "4.3.9",
    "vite-plugin-dts": "2.3.0",
    "vitest": "0.32.2"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier -w"
    ]
  }
}
